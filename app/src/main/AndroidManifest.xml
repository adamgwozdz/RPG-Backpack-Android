<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wodu.mobile.rpg_backpack">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Application"
        android:allowBackup="false"
        android:icon="@drawable/backpack_128px"
        android:label="@string/app_name"
        android:roundIcon="@drawable/backpack_128px"
        android:supportsRtl="true"
        android:theme="@style/Theme.RPG_Backpack">
        <activity
            android:name="android.app.Application"/>
        <activity
            android:name=".authorization.RegisterActivity"
            android:exported="true"
            android:theme="@style/Theme.RPG_Backpack.NoActionBar"/>
        <activity
            android:name=".authorization.LoginActivity"
            android:exported="true"
            android:theme="@style/Theme.RPG_Backpack.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.RPG_Backpack.NoActionBar" />
    </application>

</manifest>